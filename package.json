{
  "name": "performance-nodejs",
  "version": "1.0.0",
  "description": "Estudo e teste de performance com Artillery no NodeJS",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node ./src/server.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:run",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/data-source.ts",
    "build": "rm -rf ./dist && tsc",
    "start": "node ./dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.1",
    "bcryptjs": "2.4.3",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.3.17",
    "typeorm-extension": "3.0.1",
    "uuid": "9.0.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/bcryptjs": "2.4.3",
    "@types/express": "4.17.17",
    "@types/node": "20.6.0",
    "@types/uuid": "9.0.3",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
